<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.imwxz.store.entity">

    <class name="CartEntity" table="cart" schema="store">
        <id name="cartId">
            <column name="cart_id" sql-type="int(11)"/>
            <generator class="increment"/>
        </id>
        <property name="userId">
            <column name="user_id" sql-type="int(11)"/>
        </property>
        <property name="bookId">
            <column name="book_id" sql-type="int(11)"/>
        </property>
        <property name="cartAmount">
            <column name="cart_amount" sql-type="int(11)"/>
        </property>
        <many-to-one name="userByUserId" class="com.imwxz.store.entity.UserEntity" insert="false" update="false">
            <column name="user_id"/>
        </many-to-one>
        <many-to-one name="bookByBookId" class="com.imwxz.store.entity.BookEntity" insert="false" update="false">
            <column name="book_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>